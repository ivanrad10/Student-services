<Window x:Class="GUI.View.DodajNepolozeniPredmet"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GUI.View"
        mc:Ignorable="d"
        Title="DodajNepolozeniPredmet" Height="450" Width="800">
    <Grid>
        <ListBox Name="ListBoxPredmeti" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="20" Width="700" Height="300">
        </ListBox>
        
        <Button x:Name="Potvrdi"
            Content="Potvrdi"
            Click="PotvrdiOnCLick"
            HorizontalAlignment="Center"
            VerticalAlignment="Bottom"
            Margin="10,10,200,20"/>

        <Button x:Name="Odustani"
            Content="Odustani"
            Click="OdustaniOnClick"
            HorizontalAlignment="Center"
            VerticalAlignment="Bottom"
            Margin="200,10,10,20"/>
    </Grid>
</Window>
